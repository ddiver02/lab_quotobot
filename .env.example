# Copy this file to `.env` and fill in secrets.

# Google Generative AI API key
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY

# Supabase project URL (safe to expose)
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co

# Supabase Service Role Key (server-side only, keep secret)
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Option A (Vercel Web -> Cloud Run API):
# Comma-separated list of allowed origins for CORS.
# Example: https://your-app.vercel.app,https://preview-your-app.vercel.app
CORS_ORIGINS=
